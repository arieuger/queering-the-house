{
  "name": "pmtiles-cloudflare",
  "version": "0.0.1",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@cloudflare/workers-types": "^4.20230518.0",
    "pmtiles": "^3.0.7",
    "tsx": "^4.7.0",
    "typescript": "^4.9.5",
    "wrangler": "3.19.0"
  },
  "private": true,
  "scripts": {
